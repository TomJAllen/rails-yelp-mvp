<h1>Restaurants</h1>

<%= link_to "+ New Restaurant", new_restaurant_path%>


<ul>
 <% @restaurants.each do |restaurant| %>
      <li>
        <p><%= link_to restaurant.name , restaurant_path(restaurant)%></p>
        <p><%= restaurant.address %></p>
        <p><%= restaurant.category %></p>
      </li>
    <% end %>
</ul>
